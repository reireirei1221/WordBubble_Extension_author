{
    "name": "WordBubble_Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "WordBubble_Extension",
    "content_scripts": [
        {
            "matches": [
                "https://dl.acm.org/doi/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}